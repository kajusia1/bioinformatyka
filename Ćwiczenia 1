# Zadanie 1
import re
s = "ATGCGCGCGGCGGATCGAATAGGCTAGCTAGGCTATATGCGCTATATGCGCCGTATAGGAGTCTAGCGTATATGCGATTCGATTCGTATAGCGTAGCTCTGAGACTCTGAGAGC"
t2=re.findall('(?=(G[^A]{2}TA))',s)   #pytajnik wyszukuje liczbe nakładających; dodatkowy () wypluwa też wyniki
print(t2)

# %%
# Zadanie 2
f = open("ec.fasta", "r")  #pobieram genom E.coli #HG738867.1
g = f.read()
f.close()

g[:500]
g = g.replace('\n', '') #kasuje \n (znaki końca linii)
N = len(g)
print(N)

# %%
#Stosunki A/T i G/C

g.count("A")/g.count("T"), g.count("G")/g.count("C")

# %%
#tworzę liste trinukleotydow

nukleotydy = ["A","T","G","C"]
tri =[]

for n in nukleotydy:
    for m in nukleotydy:
        for l in nukleotydy:
            tri.append(n+m+l)

print(tri)

# %%
#tworzę liste tetranukleotydow

nukleotydy = ["A","T","G","C"]
tetra =[]

for n in nukleotydy:
    for m in nukleotydy:
        for l in nukleotydy:
            for k in nukleotydy:
                tetra.append(n+m+l+k)

print(tetra)

# %%
#częstosci występowania trinukleotydow

Z3 = [len(re.findall('(?='+x+')', g))/(N-2) for x in tri] 

print(Z3)
sum(Z3)

# %%
#częstosci występowania tetranukleotydow

Z4 = [len(re.findall('(?='+x+')', g))/(N-3) for x in tetra] 

print(Z4)

sum(Z4)

# %%
czestosci3 = dict(zip(tri,[round(x,4) for x in Z3]))

print(czestosci3)

# %%
czestosci4 = dict(zip(tetra,[round(x,6) for x in Z4]))

print(czestosci4)

# %%
def REV_COM_DNA(seq):
    complementary_code = {"A":"T", "T":"A", "G":"C", "C":"G"}
    rev_com_seq = ''
    for elem in seq:
        rev_com_seq = complementary_code[elem] + rev_com_seq
    return rev_com_seq

# %%
N_SK3 = [x for x in tri if x != REV_COM_DNA(x)]  #wybieram tylko te trinukleotydy ktore nie są rownoczesnie odwrotną komplementarną (jak np AT)
print(N_SK3)

N_SK4 = [x for x in tetra if x != REV_COM_DNA(x)]  #wybieram tylko te tetranukleotydy ktore nie są rownoczesnie odwrotną komplementarną (jak np AT)
print(N_SK4)

# %%
import matplotlib.pyplot as plt
def unikatowe(N_SK, czestosci):
    unikatowe = set() 
    ile = []
    for x in N_SK:
        for y in N_SK:
            if x == REV_COM_DNA(y) and x not in unikatowe and y not in unikatowe:
                ile.append(czestosci[x]/czestosci[y])
                unikatowe.add(x)
                unikatowe.add(y)
    print(unikatowe)
    print(ile)
    plt.hist(ile)
    plt.show()

# %%
print("unikatowe częstości dla tri")
unikatowe(N_SK3, czestosci3)
print("unikatowe częstości dla tetra")
unikatowe(N_SK4, czestosci4)


