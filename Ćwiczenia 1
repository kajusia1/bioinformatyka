#Zadanie6
import re
spek = ""
a=len(re.findall('(?=D.{2}D.)',spek)) #DXXDX 
b=len(re.findall('(?=E.{2}D.)',spek)) #EXXDX
c=len(re.findall('(?=D.{2}E.)',spek)) #DXXEX
d=len(re.findall('(?=E.{2}E.)',spek)) #EXXEX
print(a+b+c+d)

# %%
a1=len(re.findall('(?=L..)',spek))
b1=len(re.findall('(?=V..)',spek))
c1=len(re.findall('(?=I..)',spek))
print(a1+b1+c1)

